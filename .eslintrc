{
  "root": true,
  "parser": "babel-eslint",
  "plugins": [
    "react-app",
    "flowtype"
  ],
  "extends": "eslint:recommended",
  "rules": {
    'no-undef': 1,
    semi: [2, 'never'],
    'object-curly-spacing': [2, 'never'],
  },
  globals: {
  },
  env: {
    "browser": true,
    "node": true,
    "es6": true
  },
  "parserOptions": {
    "sourceType": "module"
  }
}
